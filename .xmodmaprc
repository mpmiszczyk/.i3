! comments: this is just example and i need to set it up on manually
! on each computer

! switch caps-lock as control

clear Lock
keycode 66 = Control_L
keycode 105 = Control_R
add Control = Control_L
add Control = Control_R


! volume control from hardbuttons
keycode 174 = XF86AudioLowerVolume
keycode 176 = XF86AudioRaiseVolume
