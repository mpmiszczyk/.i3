Xft.dpi: 96
Xft.autohint: 1
Xft.hintstyle:  hintlight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: none

xterm*termName: xterm-256color
xterm*locale: true

! xterm*faceName: xft:Iosevka
! xterm*faceStyle: Medium
! xterm*facePixelSize: 10
! xterm*renderFont: true

xterm*font: xft:Iosevka:antialias=true:pixelsize=13

! make Alt-f work
*xterm*metaSendsEscape: true

! increase / decrease font size in terminal
xterm*VT100.Translations: #override \
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(d)


! Molokai theme
*xterm*background: #101010
*xterm*foreground: #d0d0d0
*xterm*cursorColor: #d0d0d0
*xterm*color0: #101010
*xterm*color1: #960050
*xterm*color2: #66aa11
*xterm*color3: #c47f2c
*xterm*color4: #30309b
*xterm*color5: #7e40a5
*xterm*color6: #3579a8
*xterm*color7: #9999aa
*xterm*color8: #303030
*xterm*color9: #ff0090
*xterm*color10: #80ff00
*xterm*color11: #ffba68
*xterm*color12: #5f5fee
*xterm*color13: #bb88dd
*xterm*color14: #4eb4fa
*xterm*color15: #d0d0d0



Emacs.FontBackend: xft
Emacs.Font : Iosevka-10.5
